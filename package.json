{
  "name": "@bryce94/json-to-ts",
  "version": "1.0.6",
  "description": "将json转为ts声明",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "packageManager": "pnpm@7.0.0",
  "homepage": "https://github.com/BryceXu94/json-to-ts",
  "publishConfig": {
    "access": "public"
  },
  "type": "commonjs",
  "scripts": {
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "dev": "ts-node ./tests/debug.ts",
    "build": "rollup --bundleConfigAsCjs -c rollup.config.js",
    "lint": "eslint './src/**/*.{js,ts}'",
    "lint:fix": "eslint --fix './src/**/*.{js,ts}'",
    "publish": "pnpm run build && changeset publish"
  },
  "keywords": [
    "json-to-typescript",
    "json-to-ts",
    "将json转为ts声明"
  ],
  "author": "bryce94",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.16.0",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "rollup": "^3.21.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "change-case": "^4.1.2",
    "prettier": "^2.8.8"
  }
}
